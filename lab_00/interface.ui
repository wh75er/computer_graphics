<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="main_frame">
    <property name="background">#eca694</property>
    <property name="cursor">center_ptr</property>
    <property name="height">600</property>
    <property name="padx">5</property>
    <property name="pady">5</property>
    <property name="relief">flat</property>
    <property name="width">1000</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Listbox" id="lsbox_1">
        <property name="width">14</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="ipady">40</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="input">
        <property name="background">#d9cbd6</property>
        <property name="cursor">xterm</property>
        <property name="exportselection">true</property>
        <property name="textvariable">string:_input_text</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="ipadx">40</property>
          <property name="ipady">5</property>
          <property name="padx">10</property>
          <property name="pady">4</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="add">
        <property name="command">add_on_click_button</property>
        <property name="text" translatable="yes">add</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">ne</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="remove">
        <property name="command">remove_on_click_button</property>
        <property name="text" translatable="yes">remove</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">2</property>
          <property name="ipadx">15</property>
          <property name="padx">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="clean">
        <property name="command">clean_on_click_button</property>
        <property name="text" translatable="yes">clean</property>
        <layout>
          <property name="column">3</property>
          <property name="padx">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="randomize">
        <property name="text" translatable="yes">randomize</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="ipadx">40</property>
          <property name="pady">4</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="build">
        <property name="text" translatable="yes">build</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="ipadx">57</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="quit">
        <property name="command">quit_on_button_click</property>
        <property name="foreground">#ff0000</property>
        <property name="text" translatable="yes">QUIT</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">20</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Canvas" id="canvas">
        <property name="height">600</property>
        <property name="width">700</property>
        <layout>
          <property name="column">5</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">21</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="debug">
        <property name="anchor">w</property>
        <property name="background">#ffffff</property>
        <property name="compound">top</property>
        <property name="cursor">based_arrow_down</property>
        <property name="height">10</property>
        <property name="height">10</property>
        <property name="justify">left</property>
        <property name="relief">sunken</property>
        <property name="text" translatable="yes">| Welcome! waiting for commands...
| Add(x y splited by spaces)
| remove(choice string in lsbox to del it)
| clean(clean all dots)
</property>
        <property name="textvariable">string:_debug_text</property>
        <property name="width">26</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="ipadx">16</property>
          <property name="pady">15</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Listbox" id="lsbox_2">
        <property name="width">14</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">2</property>
          <property name="ipady">40</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Radiobutton" id="array_1">
        <property name="value">1</property>
        <property name="variable">int:_lsbox_now</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Radiobutton" id="array_2">
        <property name="value">2</property>
        <property name="variable">int:_lsbox_now</property>
        <layout>
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
